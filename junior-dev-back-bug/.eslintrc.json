{
  "env": {
    "node": true,
    "commonjs": false,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "indent": "off",
    "linebreak-style": ["error", "unix"],
    "quotes": ["error"],
    "semi": ["error", "always"]
  }
}
